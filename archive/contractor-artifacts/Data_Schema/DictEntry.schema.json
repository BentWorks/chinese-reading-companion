{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DictEntry",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "simp": {
      "type": "string"
    },
    "trad": {
      "type": "string"
    },
    "pinyin": {
      "type": "string"
    },
    "senses": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "gloss": {
            "type": "string"
          }
        }
      }
    },
    "hskLevel": {
      "type": "integer",
      "minimum": 1
    },
    "frequencyRank": {
      "type": "integer",
      "minimum": 1
    },
    "flags": {
      "type": "object",
      "properties": {
        "chengyu": {
          "type": "boolean"
        },
        "properNoun": {
          "type": "boolean"
        }
      }
    }
  },
  "required": [
    "id",
    "simp",
    "pinyin",
    "senses"
  ]
}