{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Difficulty",
  "type": "object",
  "properties": {
    "coverage": {
      "type": "number"
    },
    "unknownPer100Chars": {
      "type": "number"
    },
    "freqScore": {
      "type": "number"
    },
    "idiomDensity": {
      "type": "number"
    },
    "label": {
      "type": "string",
      "enum": [
        "too easy",
        "just right",
        "stretch",
        "too hard"
      ]
    },
    "explanation": {
      "type": "string"
    },
    "score": {
      "type": "number"
    }
  },
  "required": [
    "coverage",
    "label",
    "score"
  ]
}